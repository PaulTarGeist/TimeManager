CAAC,CAAC;EAChB","file":"/var/www/html/theme2/theme2/src/components/CurrentWeather.vue","sourceRoot":"","sourcesContent":["<script>\nimport moment from 'moment'\n\nexport default {\n  name: 'CurrentWeather',\n  city: \"\",\n  temperature: 0,\n  data() {\n    return {\n      cityWeathers : [\n        {\n            'name' : 'Lyon',\n        },\n        {\n            'name' : 'Angers',\n        },\n                {\n            'name' : 'Paris',\n        },\n                {\n            'name' : 'Villeneuve-lagarenne',\n        },\n      ],\n      city : '',\n      temperature : 0\n    }\n  },\n  methods: {\n      refreshCurrentWeather() {\n        return Math.floor(Math.random() * 50);\n      },\n      getIndexOfSelectedOption() {\n        return document.getElementById('cities').selectedIndex;\n      },\n      getFormatedDate() {\n        return moment().format('Do MMMM YYYY')\n      }\n  }\n}\n</script>\n\n<template>\n  <section>\n    <form>\n      <div class=\"form-group\">\n        <div id=\"CurrentWeather\">\n          OU ?\n          <select id=\"cities\" v-model=\"city\" class=\"form-control form-control-lg\">\n            <option v-for=\"(data) in cityWeathers\" :key=\"data.name\">{{ data.name }}</option>\n          </select>\n        </div>\n      </div>\n      <div class=\"form-group\">\n        <input v-on:click=\"refreshCurrentWeather()\" class=\"btn btn-primary\" type=\"button\" value=\"Search\" />\n      </div>\n    </form>\n    <div v-if=\"city\" class=\"city-container\">\n        <h2>Météo actuelle à {{ city }}</h2><br />\n        Température {{ refreshCurrentWeather() }}C°<br /><br />\n        Date : {{ getFormatedDate() }}\n    </div>\n  </section>\n</template>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n  form,\n  .city-container {\n    background-color:#eaeaea;\n    margin:0;\n    padding: 20px;\n    margin-bottom:20px;\n  }\n\n  h3 {\n    margin: 40px 0 0;\n  }\n\n  ul {\n    list-style-type: none;\n    padding: 0;\n  }\n\n  li {\n    display: inline-block;\n    margin: 0 10px;\n  }\n  \n  a {\n    color: #42b983;\n  }\n</style>\n"]}]}